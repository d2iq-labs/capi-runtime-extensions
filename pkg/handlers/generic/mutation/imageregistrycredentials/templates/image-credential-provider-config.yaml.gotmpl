apiVersion: kubelet.config.k8s.io/v1beta1
kind: CredentialProviderConfig
providers:
- name: {{ .ProviderBinary }}
  {{- with .ProviderArgs }}
  args:
  {{- range . }}
  - {{ . }}
  {{- end }}
  {{- end }}
  matchImages:
  - {{ printf "%q" .MirrorHost }}
  - "*"
  - "*.*"
  - "*.*.*"
  - "*.*.*.*"
  - "*.*.*.*.*"
  - "*.*.*.*.*.*"
  defaultCacheDuration: "0s"
  apiVersion: {{ .ProviderAPIVersion }}
